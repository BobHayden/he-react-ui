{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      }
    },
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true,
      "jest": true
    },
    "rules": {
      "react/require-default-props": 0,
      "import/extensions": 0,
      "react/forbid-prop-types": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "react/no-array-index-key": 1,
      "one-var": 0,
      "import/first": 1,
      "react/prop-types": 1,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ]
    }
  }